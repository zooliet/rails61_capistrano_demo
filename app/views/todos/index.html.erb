<div class='container'>
  <h1>Todos</h1>

  <%= turbo_stream_from 'todos' %>

  <%= turbo_frame_tag 'todos' do %>
    <%= render @todos %>
  <% end %>

  <%#= link_to 'New Todo', new_todo_path %>
  <%= turbo_frame_tag 'new_todo', src: new_todo_path, target: '_top' do %>
    <span><%= Time.now %></span>
  <% end %>

</div>
